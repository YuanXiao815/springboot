<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cms.mapper.accredit.IAccreditMapper">

	<insert id="insertUser" parameterType="params" useGeneratedKeys="true"
		keyProperty="id" keyColumn="id">
		INSERT INTO cm_user (
		username,
		account,
		password
		)
		VALUES
		(#{username},#{account},#{password})
	</insert>


</mapper>





